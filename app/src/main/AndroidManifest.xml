            android:name=".view.LeaderActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".views.ListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".views.CraftsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".view.CraftActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".view.CraftTagActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".views.ListCraftActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".views.CraftwsActivity"
            android:screenOrientation="portrait" />
